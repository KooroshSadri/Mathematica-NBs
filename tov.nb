   RowBox[{
    FractionBox["\[Alpha]", 
     RowBox[{"\[Gamma]", "-", "1"}]], 
    SuperscriptBox["\[Psi]", "\[Gamma]"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e4a92f07-ef1b-4328-a7bb-9abb1aa9e98f"],
